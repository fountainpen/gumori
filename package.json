{
  "name": "gumori",
  "type": "module",
  "version": "0.0.1",
  "license": "WTFPL",
  "repository": "importantimport/gumori",
  "homepage": "https://github.com/importantimport/gumori",
  "bugs": "https://github.com/importantimport/gumori/issues",
  "author": "Ëóç+85CD",
  "scripts": {
    "dev": "npm-run-all -p -r dev:unocss \"dev:astro {@} \" --",
    "dev:astro": "astro dev --experimental-integrations",
    "dev:unocss": "unocss \"src/**/*.astro\" -o ./src/styles/uno.css -w",
    "build": "pnpm build:unocss && pnpm build:astro",
    "build:astro": "astro build --experimental-integrations",
    "build:unocss": "unocss \"src/**/*.astro\" -o ./src/styles/uno.css",
    "preview": "astro preview",
    "lint": "prettier .",
    "format": "prettier -w ."
  },
  "devDependencies": {
    "@unocss/reset": "^0.37.4",
    "astro": "^1.0.0-beta.40",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "prettier-plugin-astro": "^0.0.12",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-pretty-code": "^0.3.1",
    "rehype-slug": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "shiki": "^0.10.1",
    "unocss": "^0.37.4",
    "vite-plugin-pwa": "0.11.11"
  },
  "dependencies": {
    "@astrojs/rss": "^0.2.0",
    "@astrojs/sitemap": "^0.1.0",
    "astro-compress": "^0.0.8"
  }
}